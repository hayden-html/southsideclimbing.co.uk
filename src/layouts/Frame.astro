---
import BaseHead from "../components/frame/BaseHead.astro";
import Nav from "../components/frame/nav/Nav.astro";
import Footer from "../components/frame/Footer.astro";

const { title, description } = Astro.props;
---

<!doctype html>
<html lang="en">
    <head>
        <BaseHead title={title} description={description} />
        <meta name="ExtraHead" />
        <link rel="sitemap" href="/sitemap-index.xml" />
    </head>
    <body>
        <Nav />
        <main>
            <slot />
        </main>
        <Footer />
    </body>
</html>
